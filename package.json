{
  "name": "tracking-monorepo",
  "version": "1.0.0",
  "description": "埋点监控系统Monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r run dev",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "start": "pnpm --parallel -r run start",
    "clean": "pnpm -r exec rm -rf node_modules && pnpm -r exec rm -rf dist",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "type-check": "pnpm -r run type-check",
    "backend:dev": "pnpm --filter=@tracking/backend run dev",
    "backend:start": "pnpm --filter=@tracking/backend run start",
    "frontend:dev": "pnpm --filter=@tracking/frontend run dev",
    "frontend:build": "pnpm --filter=@tracking/frontend run build",
    "sdk:build": "pnpm --filter=@tracking/tracking run build",
    "install:all": "pnpm install",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@tracking/tracking": "workspace:*",
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.48.0",
    "turbo": "^1.10.0"
  },
  "keywords": [
    "tracking",
    "analytics",
    "monitoring",
    "monorepo",
    "pnpm"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}